{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "class-methods-use-this": "off",
    "no-new": "off"
  },
  "import/no-unresolved": [
    "error",
    {
      "caseSensitive": false
    }
  ]
}
